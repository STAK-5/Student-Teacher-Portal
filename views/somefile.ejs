<!DOCTYPE HTML>
<html ng-app="app">
<head>
    <title>SOMEFILE</title>
    <link href="/bootstrap/css/bootstrap.css" rel="stylesheet" type="text/css">
    <script src="/javascripts/angular.js" type="text/javascript"></script>
    <script src="/ngApplication/ngApp.js" type="text/javascript"></script>
</head>
<html>
    <body ng-controller="controller1">
        <p class="alert alert-danger text-center"> <span> Hello <b><%= title %></b>. </span></p>
        <div class="container">
            <div class="row">
                <div class="col-md-2">
                    <button class="btn btn-block" ng-click="showname()"> {{name}} </button>
                </div>
                <div class="col-md-2">
                    <button class="btn btn-primary btn-block" ng-click="showname()"> {{name}} </button>
                </div>
                <div class="col-md-2">
                    <button class="btn btn-danger btn-block" ng-click="showname()"> {{name}} </button>
                </div>
                <div class="col-md-2">
                    <button class="btn btn-warning btn-block" ng-click="showname()"> {{name}} </button>
                </div><div class="col-md-2">
                    <button class="btn btn-default btn-block" ng-click="showname()"> {{name}} </button>
                </div>
                <div class="col-md-2">
                    <button class="btn btn- btn-block" ng-click="showname()"> {{name}} </button>
                </div>

            </div>
        </div>
    </body>
</html>